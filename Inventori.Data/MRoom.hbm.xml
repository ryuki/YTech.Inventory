<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Inventori.Data" namespace="Inventori.Data">
	<class name="MRoom" table="M_ROOM">

		<id name="RoomId" column="ROOM_ID" type="String">
			<generator class="assigned"/>
		</id>
		<property column="ROOM_NAME" type="String" name="RoomName" length="50" />
		<property column="ROOM_COMISSION" type="Decimal" name="RoomComission" />
		<property column="ROOM_DESC" type="String" name="RoomDesc" length="2147483647" />
		<property column="MODIFIED_BY" type="String" name="ModifiedBy" length="50" />
		<property column="MODIFIED_DATE" type="DateTime" name="ModifiedDate" />
		
	</class>
</hibernate-mapping>
