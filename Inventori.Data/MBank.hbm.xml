<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Inventori.Data" namespace="Inventori.Data">
	<class name="MBank" table="M_BANK">

		<id name="BankId" column="BANK_ID" type="String">
			<generator class="assigned"/>
		</id>
		<property column="BANK_NAME" type="String" name="BankName" length="50" />
		<property column="BANK_ADDRESS" type="String" name="BankAddress" length="100" />
		<property column="BANK_LIMIT_GIRO_PER_MONTH" type="Decimal" name="BankLimitGiroPerMonth" />
		<property column="MODIFIED_BY" type="String" name="ModifiedBy" length="50" />
		<property column="MODIFIED_DATE" type="DateTime" name="ModifiedDate" />
		
	</class>
</hibernate-mapping>
