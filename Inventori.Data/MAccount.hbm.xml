<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Inventori.Data" namespace="Inventori.Data">
	<class name="MAccount" table="M_ACCOUNT">

		<id name="AccountId" column="ACCOUNT_ID" type="String">
			<generator class="assigned"/>
		</id>
		<property column="ACCOUNT_NAME" type="String" name="AccountName" length="50" />
		<property column="ACCOUNT_STATUS" type="String" name="AccountStatus" length="50" />
		<property column="ACCOUNT_SALDO" type="Decimal" name="AccountSaldo" />
		<property column="ACCOUNT_DESC" type="String" name="AccountDesc" length="50" />
		<property column="ACCOUNT_POSITION" type="String" name="AccountPosition" length="50" />
		<property column="MODIFIED_BY" type="String" name="ModifiedBy" length="50" />
		<property column="MODIFIED_DATE" type="DateTime" name="ModifiedDate" />
		
	</class>
</hibernate-mapping>
