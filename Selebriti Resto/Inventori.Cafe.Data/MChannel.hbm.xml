<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Inventori.Cafe.Data" namespace="Inventori.Cafe.Data">
	<class name="MChannel" table="M_CHANNEL">

		<id name="ChannelId" column="CHANNEL_ID" type="String">
			<generator class="assigned"/>
		</id>
		<property column="CHANNEL_NAME" type="String" name="ChannelName" length="50" />
		<property column="CHANNEL_ADDRESS" type="String" name="ChannelAddress" length="100" />
		<property column="CHANNEL_PHONE" type="String" name="ChannelPhone" length="50" />
		<property column="CHANNEL_FAX" type="String" name="ChannelFax" length="50" />
		<property column="CHANNEL_STATUS" type="String" name="ChannelStatus" length="50" />
		<property column="CHANNEL_NPWP" type="String" name="ChannelNpwp" length="50" />
		<property column="CHANNEL_DESC" type="String" name="ChannelDesc" length="50" />
		<property column="CHANNEL_DESC2" type="String" name="ChannelDesc2" length="50" />
		<property column="MODIFIED_BY" type="String" name="ModifiedBy" length="50" />
		<property column="MODIFIED_DATE" type="DateTime" name="ModifiedDate" />
		
	</class>
</hibernate-mapping>
