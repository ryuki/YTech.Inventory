<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Inventori.Cafe.Data" namespace="Inventori.Cafe.Data">
	<class name="TCafeSetting" table="T_CAFE_SETTING">

		<id name="SettingId" column="SETTING_ID" type="String">
			<generator class="assigned"/>
		</id>
		<property column="EXPORTED_DIR" type="String" name="ExportedDir" length="100" />
		<property column="DISCOUNT_PASSWORD" type="String" name="DiscountPassword" length="50" />
		<property column="TELP_NO_SARAN_KRITIK" type="String" name="TelpNoSaranKritik" length="50" />
		<property column="MODIFIED_BY" type="String" name="ModifiedBy" length="50" />
		<property column="MODIFIED_DATE" type="DateTime" name="ModifiedDate" />
		
	</class>
</hibernate-mapping>
