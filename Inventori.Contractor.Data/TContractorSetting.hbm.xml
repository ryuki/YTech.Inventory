		<id name="SettingId" column="SETTING_ID" type="String">
			<generator class="assigned"/>
		</id>
		<property column="GIRO_DELETE_PIN" type="String" name="GiroDeletePin" length="50" />
		<property column="PO_DELETE_PIN" type="String" name="PoDeletePin" length="50" />
		<property column="LOGO_IMAGE" type="BinaryBlob" name="LogoImage" />
		<property column="COMPANY_NAME_FONT" type="String" name="CompanyNameFont" length="50" />
		<property column="COMPANY_NAME_COLOUR" type="String" name="CompanyNameColour" length="50" />
		<property column="COMPANY_NAME_FONT_HEIGHT" type="Int32" name="CompanyNameFontHeight" />
		
	</class>
</hibernate-mapping>
